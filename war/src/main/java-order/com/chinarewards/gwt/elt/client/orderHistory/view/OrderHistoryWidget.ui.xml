<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="new1.css" field="new1"></ui:style>
	<g:HTMLPanel>
		<div class="{new1.frame-wrap}">
			 <div class="breadcrumb">
               <a href="#">首页</a>-><a href="#">兑换历史</a>
             </div>

			<!--查询条件 [[ -->
			<div class="{new1.query-wrap}">
				<div class="{new1.form}">
					<label for="">订单状态:</label>
					<span class="{new1.nbsp}"></span>
					<g:ListBox ui:field="status"  styleName="{new1.select}"/>
					<span class="{new1.nbspbig}"></span>
                    <span class="{new1.nbspbig}"></span>
					<label for="">下单时间:</label>
					<d:DateBox ui:field="createTime" styleName="{new1.text}" />->
					<d:DateBox ui:field="createTimeEnd" styleName="{new1.text}" />
					<span class="{new1.nbspbig}"></span>
					
					<span class="{new1.nbspbig}"></span>
					    <span class="{new1.blue-btn}">
						<g:Button ui:field="searchBtn" text="搜索" />
					    </span>
					<span class="{new1.nbspbig}"></span>
					<span class="{new1.nbspbig}"></span>
					<span class="{new1.nbspbig}"></span>
					
				</div>
			</div>
			<!--查询条件 ]] -->


			<span class="{new1.cutline}"></span>
			<div class="{new1.divtext}">一共有 <g:InlineLabel ui:field="dataCount" styleName="{new1.divfont}"></g:InlineLabel> 条订单</div><div  class="{new1.divend}"></div>
			<div class="{new1.table}">
				<g:FlowPanel ui:field="resultPanel" />
			</div>


			<!--分页 [[ -->
			<div id="rewardlist_page2" class="{new1.pager}">
				<div class="{new1.pager-left}">
					每页显示条数
					<select class="{new1.select}">
						<option>10</option>
					</select>
				</div>

				<div class="{new1.pager-right}">
					<g:FlowPanel ui:field="resultpage" />
				</div>
			</div>
			<!--分页 ]] -->

		</div>
	</g:HTMLPanel>
</ui:UiBinder>