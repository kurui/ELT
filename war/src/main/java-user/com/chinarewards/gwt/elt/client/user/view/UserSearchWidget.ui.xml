<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client">
	<ui:image field="button_search" resource="button_search.jpg"></ui:image>
	<ui:image field="button_mod" resource="button_mod.jpg"></ui:image>
	<ui:image field="button_act" resource="button_act.jpg"></ui:image>
	<ui:image field="button_log" resource="button_log.jpg"></ui:image>
	<ui:image field="button_reset" resource="button_reset.jpg"></ui:image>
	<ui:image field="button_add" resource="button_add.jpg"></ui:image>
	<ui:image field="button_chose" resource="button_chose.jpg"></ui:image>
	<ui:image field="post_bg" resource="post_bg.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:style src="myMain.css"></ui:style>
	<ui:style src="user.css" field="other"></ui:style>
	<g:HTMLPanel>
	
	           	<div class="{style.fmtable}">
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                      <tr>
                        <td width="12%" class="{style.title}">账号名称：</td>
                        <td width="21%" class="{style.intext}"><g:TextBox ui:field="accountName" /></td>
                        <td width="12%" class="{style.title}">手机号码：</td>
                        <td width="16%" class="{style.intext}"><g:TextBox ui:field="mobile" /></td>
                        <td width="10%" class="{style.title}">电子邮箱：</td>

                        <td width="14%" class="{style.intext}"><g:TextBox ui:field="email" /></td>
                        <td width="15%" class="{style.caozuo}"><g:Button ui:field="search" text="查询" styleName="button-search"/></td>
                      </tr>
                      <tr>
                        <td width="12%" class="{style.title}">关联企业名称：</td>
                        <td width="21%" class="{style.intext}">
                        	<g:TextBox ui:field="enterpriseName" />
							<g:Button ui:field="choose" styleName="button-chose">选择</g:Button>
						</td>
                        <td width="12%" class="{style.title}">状　　态：</td>
                        <td width="16%" class="{style.intext}"><g:ListBox ui:field="status" /></td>
                        <td width="10%" class="{style.title}"></td>
                        <td width="14%" class="{style.intext}"></td>
                        <td width="15%" class="{style.caozuo}"><g:Button ui:field="reset" text="重置" styleName="button-reset"/></td>
                      </tr>
                    </table> 
                 </div>
                 <div class="{style.listtable}">
					<g:FlowPanel ui:field="result">
					</g:FlowPanel>
				 </div>   
				 <div id="post">
				 	<g:Button ui:field="add" text="新增" styleName="button-add"/>
				 	<g:Button ui:field="Update" text="修改" styleName="button-search"/>
				 	<g:Button ui:field="active" text="激活" styleName="button-act"/>
					<g:Button ui:field="logOff" text="注销" styleName="button-log"/>
					<g:Button ui:field="resetPwd" text="密码重置" styleName="button-modp"/>
					<!--<g:Button ui:field="searchSubAccount" text="查看子账号" styleName="button-modp" />
				 -->
				 </div>
	</g:HTMLPanel>
</ui:UiBinder>