<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="myMain.css"></ui:style>
	<ui:style src="elt_min.css" field="new2"></ui:style>
	<g:HTMLPanel >

<div class="{new2.frame-wrap}" style="width:600px;">
			<!--查询条件 [[ -->
			<div class="{new2.query-wrap}">
				<div class="{new2.form}">
					<tr>
						
						<td class="{style.title}">搜索条件(名称，手机，邮件)：</td>
						<td class="{style.intext2}">
							<g:TextBox ui:field="name"></g:TextBox>
						</td>
						<td class="{style.caozuo}">
							<span class="{new2.btn}"><g:Button ui:field="searchBtn" >查询</g:Button></span>
							<span class="{new2.btn}"><g:Button ui:field="resetBtn" >重置</g:Button></span>
						</td>
					</tr>
				</div>
			</div>
			<span class="{new2.cutline}"></span>

			<div >
				<g:FlowPanel ui:field="result" />
			</div>

			<div class="{new2.table}">
				<table width="100%">
					<tr>
						<td>添加的员工：</td>
						<td><g:FlowPanel ui:field="specialBoxPanel" styleName="{new2.label}"/></td>
					</tr>
				</table>
			</div>
			<div style="height:10px"></div>
			<div class="{new2.query-wrap}">
				<div class="{new2.form}">
			

					<span class="{new2.btn}">
						<g:Button ui:field="chooseBtn" text="确定"></g:Button>
					</span>
					<span class="{new2.btn}">
						<g:Button ui:field="cancelBtn" text="取消"></g:Button>
					</span>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>