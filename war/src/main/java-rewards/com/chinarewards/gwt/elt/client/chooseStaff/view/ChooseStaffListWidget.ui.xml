<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="myMain.css"></ui:style>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.frame-wrap}" style="width:600px;">
			<!--查询条件 [[ -->
			<div class="{new2.query-wrap}">
				<div class="{new2.form}">
					<label for="">搜索条件(名称)：</label>
					<g:TextBox ui:field="name" styleName="{new2.text}" />


					<span class="{new2.btn}">
						<g:Button ui:field="searchBtn" text="查询" />
					</span>
					<span class="{new2.btn}">
						<g:Button ui:field="resetBtn" text="重置" />
					</span>
				</div>
			</div>
			<!--查询条件 ]] -->


			<span class="{new2.cutline}"></span>

			<div class="{new2.table}">
				<g:FlowPanel ui:field="result" />
			</div>



			<g:FlowPanel ui:field="resultpage" />

			<g:InlineLabel text="准备添加的员工：" styleName="{new2.label}" />
			<g:FlowPanel ui:field="specialBoxPanel" />



			<div class="{new2.query-wrap}">
				<div class="{new2.form}">
			

					<span class="{new2.btn}">
						<g:Button ui:field="chooseBtn" text="确定"></g:Button>
					</span>
					<span class="{new2.btn}">
						<g:Button ui:field="cancelBtn" text="取消"></g:Button>
					</span>
				</div>
			</div>

		</div>
	</g:HTMLPanel>
</ui:UiBinder>