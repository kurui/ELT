<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>

	<ui:image resource="input_text.jpg" field="input_text"
		repeatStyle="Horizontal"></ui:image>
	<ui:image resource="preview_bg.jpg" field="preview_bg"
		repeatStyle="Horizontal"></ui:image>
	<ui:image field="post_bg" resource="post_bg.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:style src="rewardsItem.css" field="item"></ui:style>
	<ui:style src="new1.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.employee}">

			<div class="{new2.content-wrap}">
				<div class="{new2.content-info}">
					<div class="{new2.breadcrumb}">
						<a href="#">首页</a>
						&gt;
						<span>查看奖项</span>
					</div>

			
					<div class="{new2.frame-wrap}">
						<div class="{new2.query-wrap}">
							<div class="{new2.form}">
								<span>
									<h2>奖项详细</h2>
								</span>
							</div>
						</div>

						<div class="{new2.form}">
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 奖项名称：</label>
								<div class="{new2.element}">
									<g:Label ui:field="rewardsName" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 奖项说明：</label>
								<div class="{new2.element}">
									<g:Label ui:field="rewardsDefinition" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 奖项条件：</label>
								<div class="{new2.element}">
									<g:Label ui:field="standard" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 获奖名额：</label>
								<div class="{new2.element}">
									<g:Label ui:field="peopleSizeLimit" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 总积分：</label>
								<div class="{new2.element}">
									<g:Label ui:field="totalJF" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 每人得奖积分：</label>
								<div class="{new2.element}">
									<g:Label ui:field="rewardsFrom" />
								</div>
							</div>
							<div id="tm">
								<div class="{new2.form-line}">
									<label class="{new2.label}"> 提名人：</label>
									<div class="{new2.element}">
										<g:FlowPanel ui:field="staffAreaPanel" />
									</div>
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}"> 候选人：</label>
								<div class="{new2.element}">
									<g:FlowPanel ui:field="staffPanel" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}">奖项模式：</label>
								<div class="{new2.element}">
									<g:Label ui:field="itemModel" />
								</div>
							</div>
							<div class="{new2.form-line}">
								<label class="{new2.label}">开始时间：</label>
								<div class="{new2.element}">
									<g:Label ui:field="startTime" />
								</div>
							</div>
							<div id="ones">
								<div class="{new2.form-line}">
									<label class="{new2.label}">预计颁奖时间：</label>
									<div class="{new2.element}">
										<g:InlineLabel ui:field="expectTime" />
										提前
										<g:InlineLabel ui:field="tmdays" />
										天提名
									</div>
								</div>
							</div>
							<div id="period">
								<div class="{new2.form-line}">
									<label class="{new2.label}">下次颁奖时间：</label>
									<div class="{new2.element}">
										<g:InlineLabel ui:field="nextRewardsTime" />
										提前
										<g:InlineLabel ui:field="tmday" />
										天提名
									</div>
								</div>
								<div class="{new2.form-line}">
									<label class="{new2.label}"> 颁奖周期： </label>
									<div class="{new2.element}">
										<g:Label ui:field="settingText" />
									</div>
								</div>

								<div class="{new2.form-line}">
									<label class="{new2.label}">自动颁奖： </label>
									<div class="{new2.element}">
										<g:CheckBox ui:field="autoCbx" />
									</div>
								</div>


								<div class="{new2.form-line}">
									<label class="{new2.label}">下次公布时间：</label>
									<div class="{new2.element}">
										<g:Label ui:field="nextPublicTime" />
									</div>
								</div>

								<div class="{new2.form-line}">
									<label class="{new2.label}">特殊条件：</label>
									<div class="{new2.element}">
										<g:CheckBox ui:field="specialCbx"></g:CheckBox>
										<g:RadioButton ui:field="birthRadio" name="condition"> 员工生日
										</g:RadioButton>
									</div>
								</div>
							</div>

							<span class="{new2.cutline}"></span>

							<div class="{new2.form-line}">
								<div class="{new2.element}">
									<span class="{new2.btn}">
										<g:Button ui:field="back" styleName="button-ok">返回 </g:Button>
									</span>
									<span class="{new2.btn}">
										<g:Button ui:field="update" styleName="button-ok">修改 </g:Button>
									</span>
								</div>
							</div>

						</div>
					</div>

				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>