<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.employee}">
			<div class="{new2.news-list}">
				<div class="{new2.news}">
					<span class="{new2.news-icon3}"></span>
					<div class="{new2.news-info}">
						<p>
						   你调戏了
							<strong>
								<g:InlineLabel ui:field="deptName" />
							</strong>
							<strong>
								<g:InlineLabel ui:field="staffName" />
							</strong>
							一下
						</p>
						<p>
							<span>
								<g:InlineLabel ui:field="createDate" />
							</span>
							<span>
								<g:Anchor text="继续调戏TA" ui:field="dallianceBtn" styleName="{new2.aa}"  />
							</span>


						</p>
	
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 