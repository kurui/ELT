<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.employee}">
			<!-- 内容区 [[ -->
			<div class="{new2.content-wrap}">
				<div class="{new2.content-info}">

					<div class="{new2.news-wrap}">
						<div class="{new2.news-content}">

							<div class="{new2.remark-list-info} {new2.clearfix}">
								<g:Image pixelSize="30,30" ui:field="photo" />
								<div class="{new2.remark-info-wrap}">
									<p>
										<span class="{new2.remark-name"><g:InlineLabel ui:field="staffName" /> </span>
										说：
										<span class="{new2.remark-content}"><g:InlineLabel ui:field="message" /></span>
									</p>
									<p>
										<span><g:InlineLabel ui:field="replyDate" /></span>
										<g:Anchor ui:field="replyBtn" text="回复" />
									</p>
								</div>
							</div>

						</div>
						<!-- 内容区 ]] -->

					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 