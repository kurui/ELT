<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:image field="button_ok" resource="button_ok.jpg"></ui:image>
	<ui:image field="button_back" resource="button_back.jpg"></ui:image>
	<ui:image field="post_bg" resource="post_bg.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:style src="confirm.css"></ui:style>
	<g:HTMLPanel>
		<div class="{style.center}">
			<g:ScrollPanel ui:field="msg"></g:ScrollPanel>
		</div>
		<div class="{style.post}">
			<g:Button ui:field="ok" text="确定" styleName="button-ok" />
			<g:Button ui:field="cancel" text="取消" styleName="button-back" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>